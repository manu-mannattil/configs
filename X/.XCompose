# vim: ft=conf fdm=marker et sts=2 sw=2

# Load locale specific compose file (e.g. the one in /usr/share/X11/locale/en_US.UTF-8).
include "%L"

# Greek letters {{{1
# ------------------

# Taken from https://gist.github.com/pkgw/5422749

# Lowercase Greek letters {{{2
# ----------------------------

<Multi_key> <g> <a> : "α"
<Multi_key> <g> <b> : "β"
<Multi_key> <g> <g> : "γ"
<Multi_key> <g> <d> : "δ"
<Multi_key> <g> <3> : "ε" # note!
<Multi_key> <g> <z> : "ζ"
<Multi_key> <g> <e> : "η"
<Multi_key> <g> <h> : "θ" # note!
<Multi_key> <g> <i> : "ι"
<Multi_key> <g> <k> : "κ"
<Multi_key> <g> <l> : "λ"
<Multi_key> <g> <m> : "μ"
<Multi_key> <g> <n> : "ν"
<Multi_key> <g> <x> : "ξ"
<Multi_key> <g> <p> : "π"
<Multi_key> <g> <r> : "ρ"
<Multi_key> <g> <s> : "σ"
<Multi_key> <g> <t> : "τ"
<Multi_key> <g> <u> : "ψ"
<Multi_key> <g> <f> : "φ" # note!
<Multi_key> <g> <c> : "χ"
<Multi_key> <g> <o> : "ω"

# Uppercase Greek letters {{{2
# ----------------------------

<Multi_key> <g> <G> : "Γ"
<Multi_key> <g> <D> : "Δ"
<Multi_key> <g> <H> : "Θ"
<Multi_key> <g> <L> : "Λ"
